{
  "manifest_version": 3,
  "name": "RSS Feed Reader",
  "version": "0.0.1",
  "description": "A simple RSS Feed Reader extension for Chrome",
  "action": {
    "default_popup": "popup.html",
    "default_icon": {
      "48": "icons/icon48.png",
      "128": "icons/icon128.png"
    }
  },
  "permissions": ["storage"],
  "icons": {
    "48": "icons/icon48.png",
    "128": "icons/icon128.png"
  },
  "background": {
    "service_worker": "background_fetchFeeds.js"
    
  },
  "options_page": "settings.html",
  "chrome_url_overrides": {
    "newtab": "newtab.html"
  },
  "content_security_policy": {
    "extension_pages": "script-src 'self'; object-src 'self'"
  }
}
